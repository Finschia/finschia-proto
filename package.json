{
  "name": "lbmjs-types",
  "version": "0.45.0",
  "main": "index.js",
  "description": "JS and TS types relating to Protocol Buffers used by LBM-SDK",
  "contributors": [
    "zemyblue <zemyblue@users.noreply.github.com>"
  ],
  "license": "Apache-2.0",
  "files": [
    "confio",
    "cosmos_proto",
    "gogoproto",
    "google",
    "ibc",
    "lbm",
    "ostracon",
    "*.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "codegen": "./scripts/codegen.sh",
    "lbmsdk_update": "git submodule update --remote --merge",
    "build": "rm -rf ./build && tsc && ./scripts/prepare.sh",
    "clean": "rm -rf ./build confio cosmos_proto gogoproto google ibc lbm ostracon"
  },
  "dependencies": {
    "long": "^4.0.0",
    "protobufjs": "~6.11.2"
  },
  "devDependencies": {
    "@types/long": "^4.0.1",
    "@types/node": "^15.6.2",
    "prettier": "^2.5.1",
    "ts-proto": "^1.92.1",
    "typescript": "~4.2"
  },
  "packageManager": "yarn@3.1.1"
}
