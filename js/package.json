{
  "name": "@finschia/finschia-proto",
  "version": "4.0.0",
  "main": "index.js",
  "description": "JS and TS types relating to Protocol Buffers used by FINSCHIA",
  "contributors": [
    "zemyblue <zemyblue@users.noreply.github.com>"
  ],
  "license": "Apache-2.0",
  "files": [
    "/proofs.*",
    "cosmos",
    "cosmos_proto",
    "cosmwasm",
    "gogoproto",
    "google",
    "ibc",
    "lbm",
    "ostracon",
    "tendermint",
    "*.md",
    "!wasmd/**/*.md",
    "!finschia-sdk/**/*.md",
    "!ibc-go/**/*.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "clear": "./scripts/clear.sh",
    "prepare": "./scripts/prepare.sh",
    "codegen": "rm -rf ./src && ./scripts/codegen.sh && yarn run format",
    "submodule_update": "git submodule update --remote --merge",
    "build": "rm -rf ./build && tsc && yarn run prepare"
  },
  "dependencies": {
    "long": "^4.0.0",
    "protobufjs": "~6.11.2"
  },
  "devDependencies": {
    "@types/long": "^4.0.1",
    "@types/node": "^15.6.2",
    "prettier": "^2.5.1",
    "ts-proto": "1.115.5",
    "typescript": "~4.2"
  },
  "packageManager": "yarn@3.1.1"
}
